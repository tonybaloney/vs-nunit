{
  "name": "vs-test-details",
  "version": "1.1.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "test": "jest --ci --testResultsProcessor=./node_modules/jest-nunit-reporter",
    "compile:prod": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "postbuild": "npm run package",
    "package": "cp README.md overview.md && tfx extension create --output-path build",
    "gallery-publish:dev": "tfx extension publish --publisher AnthonyShaw-testing --overrides-file vss-extension-testing.json --output-path build --rev-version",
    "gallery-publish:prod": "tfx extension publish --output-path build --rev-version",
    "clean": "rimraf ./dist && rimraf ./build/*.vsix",
    "go:dev": "npm run compile:dev && npm run package && npm run gallery-publish:dev",
    "go:prod": "npm run compile:prod && npm run package && npm run gallery-publish:prod"
  },
  "keywords": [],
  "author": "Anthony Shaw",
  "license": "MIT",
  "dependencies": {
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^1.160.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "vss-web-extension-sdk": "^5.141.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "jest": "^24.8.0",
    "jest-nunit-reporter": "^1.3.1",
    "rimraf": "~2.6.2",
    "sass": "^1.40.1",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.12.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  }
}
